<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>MajorVersion</key>
		<integer>8</integer>

		<key>MinorVersion</key>
		<integer>0</integer>

		<key>SiteTemplate</key>
		<dict>
			<key>Image</key>
			<string>site10_start.jpg</string>

			<key>Caption</key>
			<string>Zing</string>

			<key>Description</key>
			<string>This site includes search, results, detail and favorites pages.  In addition to the search functionality offered in the search page, the default search page contains a &quot;Browse by Category&quot; area that displays master keywords as hyperlinks. This site is designed to show a moderate amount of text from catalog fields in the results page, and to show extensive text from fields in the detail page.</string>

			<key>PageLayout</key>
			<array>
				<dict>
					<key>Global</key>
					<true/>
				</dict>
				<dict>
					<key>Search</key>
					<true/>
					<key>DefaultSearchPage</key>
					<string>Search page with categories</string>
				</dict>
				<dict>
					<key>Results</key>
					<true/>
					<key>DefaultResultsPage</key>
					<string>Results Page (single column)</string>
				</dict>
				<dict>
					<key>DetailsWeb</key>
					<true/>
					<key>DefaultDetailsWebPage</key> 
					<string>Detail Page</string>
				</dict>
				<dict>
					<key>Collections</key>
					<true/>
					<key>DefaultCollectionsPage</key>
					<string>Collection Page (single column)</string>
				</dict>
				<dict>
					<key>Publish</key>
					<true/>
				</dict>
			</array>

			<key>GlobalTemplate</key>
			<dict>
				<key>SiteTitle</key>
				<string>Portfolio NetPublish Site</string>

				<key>MainLogoGraphic</key>
				<string></string>

				<key>HeaderHTML</key>
				<string></string>

				<key>FooterHTML</key>
				<string></string>

				<key>WelcomePageHTML</key>
				<string></string>

				<key>IncludeLinkToHomePage</key>
				<true/>
				<key>HomePageURL</key>
				<string>http://www.extensis.com/</string>
			</dict>

			<key>PublishTemplate</key>
			<dict>

				<key>SelectedWebServer</key> <!-- server name that is selected -->
				<string>Server1</string>

				<key>ServerSubFolder</key>
				<string></string>

				<key>UploadOriginalsWithSite</key>
				<true/>

				<key>SiteEditingPassword</key>
				<string></string>
				<key>SiteViewingPassword</key>
				<string></string>
			</dict>
		</dict>
	</dict>
</plist>
