<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>MajorVersion</key>
		<integer>8</integer>
		<key>MinorVersion</key>
		<integer>0</integer>
		<key>PageTemplate</key>
		<dict>
			<key>Image</key>
			<string>site10_detail.jpg</string>
			<key>Caption</key>
			<!-- Captions should be unique to PageTemplates -->
			<string>Detail Page</string>
			<key>Description</key>
			<string>This page option is designed with a 256-pixel version of the image at the top of the page and field information below the image. The Description field appears as a title element and is not modifiable via the NetPublish Assistant. This option contains a graphic for adding to favorites by default. Description, Filename, Keywords and Cataloged fields are displayed by default.  </string>
			<key>DetailsWeb</key>
			<!-- This should match PageType -->
			<dict>
				<key>ImageSource</key>
				<string>Preview</string>
				<key>ShowDownloadOriginal</key>
				<false/>
				<key>OnClickAction</key>
				<string>Display original</string>
				<key>ImageSize</key>
				<string>256</string>
				<key>StyleSheet</key>
				<string>Bravado.css</string>
				<key>ShowCollectionAddDeleteOptionsEnabled</key>
				<true/>
				<key>ShowCollectionAddDeleteOptions</key>
				<true/>
				<key>DisplayFields</key>
				<array>
					<string>Description</string><string>Filename</string><string>Keywords</string><string>Cataloged</string>
				</array>
			</dict>
		</dict>
	</dict>
</plist>
