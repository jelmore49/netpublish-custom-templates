<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>MajorVersion</key>
		<integer>8</integer>
		<key>MinorVersion</key>
		<integer>0</integer>
		<key>PageTemplate</key>
		<dict>
			<key>Image</key>
			<string>site10_results.jpg</string>
			<key>Caption</key>
			<string>Results Page (multiple columns)</string>
			<key>Description</key>
			<string>This page option contains 112-pixel thumbnails laid out in 4 columns by 10 rows. This page includes the text from the Description field shown as a title element. This title is a hyperlink, which take the user to the details page for the item. This element is not modifiable via the NetPublish Assistant. Text from Filename, Keywords and Cataloged  fields, and a graphic for adding to favorites are shown by default. In addition, a graphic for viewing detail is shown.</string>
			<key>Results</key>
			<!-- This should match PageType -->
			<dict>
				<key>SummarizeSearchCriteria</key>
				<false/>
				<key>SortResults</key>
				<true/>
				<key>SortSelection</key>
				<string>Filename</string>
				<key>RowsEnabled</key>
				<true/>
				<key>Rows</key>
				<integer>10</integer>
				<key>ColumnsEnabled</key>
				<true/>
				<key>Columns</key>
				<integer>4</integer>
				<key>ThumbnailSizeEnabled</key>
				<true/>
				<key>ThumbnailSize</key>
				<string>112</string>
				<key>OnClickAction</key>
				<string>Show details</string>
				<key>StyleSheet</key>
				<string>Bravado.css</string>
				<key>ShowCollectionAddDeleteOptionsEnabled</key>
				<true/>
				<key>ShowCollectionAddDeleteOptions</key>
				<true/>
				<key>DisplayFields</key>
				<array>
					<string>Filename</string><string>Keywords</string><string>Cataloged</string>
				</array>
			</dict>
		</dict>
	</dict>
</plist>
